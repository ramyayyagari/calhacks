<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.calhacks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReviewForm"
            android:exported="true"/>
        <activity android:name=".SignupActivity"
            android:exported="true"/>
        <activity android:name=".LoginActivity"
            android:exported="true"/>
        <activity android:name=".ViewProfile"
            android:exported="true"/>
        <activity android:name=".ViewPatientContact"
            android:exported="true"/>
        <activity android:name=".RequestForm"
            android:exported="true"/>
        <activity
            android:name=".DoctorListActivity"
            android:label="@string/title_doctor_list"
            android:exported="true"/>
        <activity
            android:name=".DoctorDetailActivity"
            android:label="@string/title_doctor_detail"
            android:parentActivityName=".DoctorListActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calhacks.DoctorListActivity" />
        </activity>
        <activity android:name=".DoctorRegistration"
            android:exported="true"/>
        <activity android:name=".DoctorPatientSelect"
            android:exported="true"/>
        <activity android:name=".PatientRegistration"
            android:exported="true"/>
        <activity android:name=".DoctorHome"
            android:exported="true"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>